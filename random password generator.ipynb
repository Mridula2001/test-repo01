    "import random\n",
    "\n",
    "lower = \"abcdefghijklmnopqrstuvwxyz\"\n",
    "upper = \"ABCDEFGHIJKLOMNOPQRSTUVWXYZ\"\n",
    "numbers = \"0123456789\"\n",
    "\n",
    "all = lower + upper + numbers\n",
    "length = 8\n",
    "password = \"\".join(random.sample(all, length))\n",
    "print(password)"
